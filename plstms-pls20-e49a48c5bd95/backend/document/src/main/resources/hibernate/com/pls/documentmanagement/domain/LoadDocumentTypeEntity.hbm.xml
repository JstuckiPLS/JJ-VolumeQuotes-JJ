<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
      <query name="com.pls.documentmanagement.domain.LoadDocumentTypeEntity.Q_GET_DOCUMENT_TYPES">
        <![CDATA[
                from LoadDocumentTypeEntity ldte where ldte.docTypeString != 'CONSIGNEE_INVOICE'
        ]]>
    </query>
</hibernate-mapping>