<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.core.domain.organization.PlsCustomerTermsEntity.Q_GET_ALL">
        <![CDATA[
                select id as key, termName as value
                from PlsCustomerTermsEntity
                order by id
        ]]>
    </query>
</hibernate-mapping>