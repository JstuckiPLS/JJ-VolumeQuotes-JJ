<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.core.domain.LookupValueEntity.Q_FIND_FOR_LOOKUP_GROUP">
        <![CDATA[
            FROM LookupValueEntity lve
            WHERE lve.lookupGroup in (:lookupGroups)
        ]]>
    </query>
</hibernate-mapping>