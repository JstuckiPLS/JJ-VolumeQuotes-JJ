<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.core.domain.organization.OrganizationNotificationsEntity.Q_GET_BY_ORG_ID">
        <![CDATA[
            FROM OrganizationNotificationsEntity orgNotification
            WHERE orgNotification.organization.id = :orgId
        ]]>
    </query>
</hibernate-mapping>