<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.shipment.domain.LoadAdditionalInfoEntity.Q_GET_ADDITIONAL_INFO_BY_LOAD_ID">
        <![CDATA[
                from LoadAdditionalInfoEntity laie
                where laie.loadId = :loadId
        ]]>
    </query>
</hibernate-mapping>