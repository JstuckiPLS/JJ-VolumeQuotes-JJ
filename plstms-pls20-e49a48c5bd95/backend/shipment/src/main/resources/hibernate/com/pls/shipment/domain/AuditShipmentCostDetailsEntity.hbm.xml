<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.shipment.domain.AuditShipmentCostDetailsEntity.Q_BY_LOAD_ID">
        <![CDATA[
                from AuditShipmentCostDetailsEntity where loadCostDetail.status = 'A' and loadCostDetail.load.id = :id
        ]]>
    </query>
</hibernate-mapping>