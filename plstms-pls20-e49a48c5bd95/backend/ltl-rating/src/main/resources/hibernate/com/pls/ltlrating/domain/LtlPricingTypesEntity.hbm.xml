<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.pls.ltlrating.domain.profile.LtlPricingTypesEntity.GET_BY_GROUP">
        <![CDATA[
           from LtlPricingTypesEntity types
           where types.groupType = :groupType
        ]]>
    </query>

    <query name="com.pls.ltlrating.domain.profile.LtlPricingTypesEntity.GET_BY_TYPE">
        <![CDATA[
           from LtlPricingTypesEntity types
           where types.ltlPricingType = :ltlPricingType
        ]]>
    </query>
</hibernate-mapping>